{
  "homepage": "https://www.sspcloud.fr",
  "name": "www.sspcloud.fr",
  "private": true,
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/garronej/keycloakify-demo-app.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -v"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "author": "u/thierryw & u/marchufschmitt & u/garronej",
  "license": "MIT",
  "keywords": [
    "sspcloud",
    "onyxia",
    "R",
    "insee"
  ],
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "6.1.0",
    "@mui/material": "6.1.0",
    "evt": "^2.5.7",
    "github-pages-plugin-for-type-route": "^2.0.4",
    "gitlanding": "^2.0.0",
    "i18nifty": "^3.2.2",
    "js-yaml": "^4.1.0",
    "memoizee": "^0.4.17",
    "moment": "^2.30.1",
    "onyxia-ui": "^5.3.3",
    "powerhooks": "^1.0.13",
    "react-async-hook": "^4.0.0",
    "tsafe": "^1.7.2",
    "tss-react": "^4.9.13",
    "type-route": "^1.1.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "@types/js-yaml": "^4.0.9",
    "@types/memoizee": "^0.4.11",
    "@types/node": "^22.5.5",
    "circular-dependency-plugin": "^5.2.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tss-unused-classes": "^1.0.2",
    "prettier": "^3.3.3",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-toolbelt": "^9.6.0"
  },
  "eslintConfig": {
    "plugins": [
      "tss-unused-classes"
    ],
    "extends": [
      "react-app"
    ],
    "rules": {
      "@typescript-eslint/no-redeclare": "off",
      "react-hooks/exhaustive-deps": "off",
      "tss-unused-classes/unused-classes": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}